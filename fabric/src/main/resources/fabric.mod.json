  ],
  "contact": {
    "homepage": "https://github.com/Cartrigger/Shutdown",
    "sources": "https://github.com/Cartrigger/Shutdown"
  },
  "license": "MIT",
  "icon": "assets/shutdown/icon.png",
  "environment": "*",
  "entrypoints": {
